<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2" schema = "public"
                   assembly = "WebAPINhibernate" namespace = "WebAPINhibernate.Models">

  <class name = "Student" table="Student">
    <id name = "ID">
      <generator class = "native">
        <param name="sequence">autoinc_pk</param>
      </generator>
    </id>

    <property name = "LastName" column="LastName"></property>
    <property name = "FirstName" column="FirstName"></property>
  </class>

</hibernate-mapping>
